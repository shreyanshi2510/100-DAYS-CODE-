//Create an enum for months and print how many days each month has.

#include <stdio.h>

int main() {
    // Enum for months
    enum Months {JANUARY, FEBRUARY, MARCH, APRIL, MAY, JUNE,
                 JULY, AUGUST, SEPTEMBER, OCTOBER, NOVEMBER, DECEMBER};

    int days[] = {31, 28, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31};
    enum Months month;

    // Print each month with number of days
    for (month = JANUARY; month <= DECEMBER; month++) {
        switch(month) {
            case JANUARY:    printf("January: %d days\n", days[month]); break;
            case FEBRUARY:   printf("February: %d days\n", days[month]); break;
            case MARCH:      printf("March: %d days\n", days[month]); break;
            case APRIL:      printf("April: %d days\n", days[month]); break;
            case MAY:        printf("May: %d days\n", days[month]); break;
            case JUNE:       printf("June: %d days\n", days[month]); break;
            case JULY:       printf("July: %d days\n", days[month]); break;
            case AUGUST:     printf("August: %d days\n", days[month]); break;
            case SEPTEMBER:  printf("September: %d days\n", days[month]); break;
            case OCTOBER:    printf("October: %d days\n", days[month]); break;
            case NOVEMBER:   printf("November: %d days\n", days[month]); break;
            case DECEMBER:   printf("December: %d days\n", days[month]); break;
        }
    }

    return 0;
}
